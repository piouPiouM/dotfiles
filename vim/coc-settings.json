{
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.messageLevel": "warning",
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "suggest.acceptSuggestionOnCommitCharacter": true,
  "suggest.echodocSupport": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.hoverTarget": "echo",
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc"
  ],
  "codeLens.enable": true,
  "diagnostic.enable": true,
  "diagnostic.virtualText": true,
  //
  // Completion priorities
  //
  "suggest.languageSourcePriority": 101,
  "snippets.priority": 90,
  "coc.source.file.priority": 10,
  "coc.source.emoji.priority": 3,
  "coc.source.around.priority": 2,
  "coc.source.buffer.priority": 1,
  //
  // UI
  //
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "⚠",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.virtualTextPrefix": "שּ ",
  "diagnostic.virtualTextLines": 3,
  "diagnostic.virtualTextLineSeparator": " ℓ.",
  "codeLens.separator": "‣",
  "suggest.snippetIndicator": " ⚡︎",
  //
  // Extensions
  //
  "coc.source.emoji.filetypes": ["markdown", "text", "textile", "rst"],
  "javascript.referencesCodeLens.enable": true,
  "javascript.implementationsCodeLens.enable": true,
  "javascript.updateImportsOnFileMove.enable": true,
  "eslint.autoFix": false,
  "eslint.autoFixOnSave": false,
  "eslint.run": "onType",
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.eslintIntegration": true,
  "snippets.ultisnips.enable": false,
  "tsserver.formatOnType": false,
  "tsserver.enableJavascript": true
}
